  Budget
  
  a=int(input())
b=int(input())
c=int(input())
d=int(input())
e=int(input())
f=a*350.34
g=b*230.90
h=c*190.55
i=d*125.30
j=e*180.90
k=f+g+h+i+j
if k<=15000:
    print("yes")
else:
    print("no")
    
Sece Dining

x=input()
y=int(input())
if(x=='front' and y==1) or (x=='rear' and y==2):
    print("left handed")
elif(x=='front' and y==2) or (x=='rear' and y==1):
    print("right handed")
else:
    print("Invalid Input")
    
Intersection of a Circle

x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=((((x1-x2)**2)+((y1-y2)**2))**(1/2))
if(r1+r2)==d:
    print("The circles are tangential")
elif(r1+r2)<d:
    print("intersect")
else:
    print("The circles overlap")
    
Microwave Oven

n=int(input("Enter the number of items\n"))
t=float(input("Enter the single item heating time\n"))
if n==1:
    print("The recommended heating time is",'%.2f' %{t})
elif n==2:
    print('The recommended heating time is',"%.2f" %((1.5)*t))
elif n==3:
    print('The recommended heating time is',"%.2f" %(2*t))
else:
    print("Number of items is more" )
