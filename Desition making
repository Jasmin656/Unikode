Greatest of two numbers

a=int(input())
b=int(input())
if a==b:
    print(f"{a} is equal to {b}")
elif a>b:
     print(f"{a} is greater than {b}")
else:
    print(f"{a} is less than {b}")

Vowel Or Consonants

character=input()
if character.isalpha()==False:
    print('not an alphabet')
elif character in 'aeiouAEIOU':
    print('vowel')
else:
    print('consonant')
    
 Grading System
 
 a=int(input())
if a==100:
    print("s")
elif a<=100 and a>=90:
    print("A")
elif a<=90 and a>=80:
    print("B")
elif a<=80 and a>=70:
    print("C")
elif a<=70 and a>=60:
    print("D")
elif a<=60 and a>=50:
    print("E")
elif a<=50:
    print("F")
else:
    print("invalid input")

Profit or Loss

x=float(input("Enter the price of a dozen mangoes\n"))
y=float(input("Enter the price at which 1 mango is being sold\n"))
output=(y*12)-x
if output==0:
    print('no profit nor loss')
elif output>0:
    p=("%.2f" %((12*y)-x))
    print('profit : Rs.',p)
else:
    l=("%.2f" %(x-(12*y)))
    print('loss : Rs.',l)
    
Fee Collection

u=str(input("Enter the student type\n"))
if u=="MSDS":
   TF=float(input("Enter tuition fee\n"))
   BF=float(input("Enter bus fee\n"))
   b=("%.2f" %(TF+BF))
   print(f"The fees to be paid by the student is Rs.{b}")
elif u=="MSH":
    TF=float(input("Enter tuition fee\n"))
    HF=float(input("Enter hostel fee\n"))
    c=("%.2f" %(TF+HF))
    print(f"The fees to be paid by the student is Rs.{c}")
elif u=="MGSDS":
    TF=float(input("Enter tuition fee\n"))
    BF=float(input("Enter bus fee\n"))
    d=("%.2f" %((1.5*TF)+Bf))
    print(f"The fees to be paid by the student is Rs.{d}")
elif u=="MGSH":
    TF=float(input("Enter tuition fee\n"))
    HF=float(input("Enter hostel fee\n"))
    e=("%.2f" %((1.5*TF)+HF))
    print(f"The fees to be paid by the student is Rs.{e}")
    
